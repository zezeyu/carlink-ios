# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'cheanda' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for cheanda
  # Architecture
    pod 'ReactorKit'
    
    # Networking
    pod 'Alamofire'
    pod 'Moya'
    pod 'Kingfisher'
    pod 'ObjectMapper'
    pod 'SwiftyJSON'

    # Rx
    pod 'RxSwift',          '~> 4.0'
    pod 'RxCocoa'
    pod 'RxAlamofire'
    pod 'RxDataSources',    '~> 3.0'
    pod 'NSObject+Rx'
    pod 'RxGesture'

    # UI
    pod 'SnapKit'
    pod 'TYCyclePagerView'
    pod 'TYPagerController'
    pod 'MJRefresh'

    # Logging
    
    # Misc.
    pod 'IQKeyboardManagerSwift'
    pod 'Then'
    pod 'ReusableKit'
    pod 'SwiftyColor'
#    pod 'MLeaksFinder'              # 检测内存泄漏
    pod 'URLNavigator'
    pod 'TTRangeSlider'

    # SDK
  target 'cheandaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'cheandaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
